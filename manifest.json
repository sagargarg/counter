{
	"name": "TabCounter2",
	"description": "Tab counting extension that allows you to set goals and shows tab statistics.",
	"icons": {  "icon1": "icon1.jpg"

	},
	"permissions": [
		"tabs",
		"notifications",
		"storage"
	],

	"background": {
			"scripts" : ["background.js"],
			"persistent": false
		},
	"browser_action": {
		"default_popup": "index.html",
		"default_icon": "icon1.jpg" 
		},

	"update_url": "http://clients2.google.com/service/update2/crx",
	"version": "1.5",
	"manifest_version": 2
}